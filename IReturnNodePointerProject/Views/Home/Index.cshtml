@model IEnumerable<IReturnNodePointerProject.Models.DatabaseControllers.Product>
@{
	ViewData["Title"] = "MovieList";
}
@{
	Layout = "_Layout";
}

<!DOCTYPE html>
<html>
	<head>
		<meta name="viewport" content="width=device-width"/>
		<title>Home Page</title>
	</head>
	<body>
		<Title >@ViewData["Title"] - Movie Store</Title>
		<h2>My Movie List</h2>
		<br />
		<thead>
			<tr>
				<th>
					@Html.DisplayNameFor(model => model.Name)
				</th>
			</tr>
		</thead>
		<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.Name)
				</td>
			</tr>			
		}
		</tbody>
	</body>
</html>
