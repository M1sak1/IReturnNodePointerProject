@model IEnumerable<>
@{
	ViewData["Title"] = "MovieList";

}
@{
	Layout = "_Layout";
}

<!DOCTYPE html>
<html>
	<head>
		<meta name="viewport" content="width=device-width"/>
		<title>Home Page</title>
	</head>
	<body>
		<h2>My Movie List</h2>
		<br />
		<tbody>
		@foreach(var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.Title)
				</td>
			</tr>
		}
		</tbody>
	</body>
</html>
