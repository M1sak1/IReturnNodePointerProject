@model IEnumerable<IReturnNodePointerProject.Models.User>
@{
    ViewData["Title"] = "WelcomeToTheBigLeagues";

}
@{
    Layout = "_Layout";
}
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Account Page</title>
</head>
<body>
	<table stye="padding: 6px;">
		<thead>
			<tr>
				<th>
					@Html.DisplayNameFor(model => model.UserID)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.UserName)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.Name)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.Email)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.IsAdmin)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.Salt)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.HashPW)
				</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{
			<tr>
				<td>
					@item.UserID
				</td>
				<td>
					@item.UserName
				</td>
				<td>
					@item.Name
				</td>
				<td>
					@item.Email
				</td>
				<td>
					@item.IsAdmin
				</td>
				<td>
					@item.Salt;
				</td>
				<td>
					@item.HashPW;
				</td>
			</tr>
		}
		</tbody>
	</table>
</body>
</html>
