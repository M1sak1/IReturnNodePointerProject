@model AdminListViewModel
@{
    Layout = "_Layout";
}
@{
    ViewData["Title"] = "AdminAccountsView";
}
<style>
	table{
		border: 1px solid;
	}
</style>
<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
</head>
<body>
	<h1>Accounts</h1>
	<table>
		<thead>
			<tr style="border:1px solid">
				<th>
					Account
				</th>
			</tr>
		</thead>
		<tbody>	
			@foreach (var item in Model.Users)
			{
				<tr>
					<td>
						@item.Name
					</td>
				</tr>
			}		
			<tr style="border:1px solid"></tr>
		</tbody>
		<tbody>
			@foreach (var item in Model.Employees)
			{
				<tr>
					<td>
						@item.Name
					</td>
				</tr>
			}
			<tr style="border:1px solid"></tr>
		</tbody>
		<tbody>
			@foreach (var item in Model.Admins)
			{
				<tr>
					<td>
						@item.Name
					</td>
				</tr>
			}
		</tbody>
	</table>
</body>
</html>
